{
    "name": "stylelint-less",
    "version": "1.0.6",
    "description": "A collection of Less specific rules for stylelint",
    "babel": {
        "presets": [
            "@babel/env"
        ]
    },
    "main": "lib/index.js",
    "scripts": {
        "prebuild": "rimraf lib",
        "build": "babel src --out-dir lib",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
    },
    "dependencies": {
        "postcss": "^8.4.14",
        "postcss-value-parser": "4.2.0"
    },
    "peerDependencies": {
        "stylelint": "^15.10.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "babel-plugin-istanbul": "^6.0.0",
        "babel-preset-jest": "^25.5.0",
        "jest": "^29.6.0",
        "postcss-less": "^6.0.0"
    },
    "author": {
        "name": "Sivantarajan",
        "url": "https://github.com/ssivanatarajan/"
    },
    "bugs": {
        "url": "https://github.com/ssivanatarajan/stylelint-less/issues"
    },
    "homepage": "https://github.com/ssivanatarajan/stylelint-less#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ssivanatarajan/stylelint-less.git"
    },
    "license": "MIT",
    "env": {
        "es6": true
    },
    "keywords": [
        "stylelint",
        "stylelint-plugin",
        "css",
        "less"
    ]
}
